rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read/write access for the next 30 days for development.
    // Make sure to update these rules before deploying to students.
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2025, 10, 2);
    }
  }
}